<h1>Comments#index</h1>
<p>Find me in app/views/comments/index.html.erb</p>
<p><%= @msg %></p>
<pre>
<table>
	<tr>
		<th>投稿#</th><th>名前</th><th>日時</th><th>区分</th><th>コメント</th></th>
	</tr>
	<% @data.each do |obj| %>
	<tr>
		<td><%= obj.id %></td>
		<td><%= obj.talker.name %></td>
		<td><%= obj.ts %></td>
		<td><%= obj.kubun %></td>
		<td><%= obj.comment %></td>
	</tr>
	<% end %>
</table>
</pre>